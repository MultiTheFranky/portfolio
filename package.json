{
    "name": "portfolio",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "dev": "astro dev --host",
        "start": "astro dev --host",
        "build": "astro build",
        "preview": "astro preview",
        "astro": "astro",
        "prepare": "husky install",
        "precommit": "pnpm lint && pnpm prettier",
        "prepush": "pnpm build",
        "lint": "eslint --ext .js,.astro src",
        "prettier": "prettier --write . --plugin=prettier-plugin-astro",
        "typecheck": "tsc --noEmit",
        "cy:run": "cypress run",
        "test:e2e": "start-server-and-test start http://localhost:4321 cy:run",
        "test": "concurrently \"pnpm typecheck\" \"pnpm lint\" \"pnpm test:e2e\" -n \"Typecheck,Lint,Cypress\""
    },
    "engines": {
        "npm": "please-use-pnpm",
        "node": ">=18",
        "pnpm": ">=8",
        "yarn": "please-use-pnpm"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@astrojs/cloudflare": "^7.7.1",
        "@astrojs/react": "^3.0.7",
        "@astrojs/tailwind": "^5.1.0",
        "@types/react": "^18.2.48",
        "@types/react-dom": "^18.2.18",
        "astro": "^3.6.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwindcss": "^3.3.6"
    },
    "devDependencies": {
        "@commitlint/cli": "18.6.0",
        "@commitlint/config-conventional": "17.8.0",
        "@typescript-eslint/parser": "^6.14.0",
        "concurrently": "^8.2.2",
        "cypress": "^13.6.2",
        "eslint": "^8.54.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-astro": "^0.31.3",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-prettier": "5.1.2",
        "eslint-plugin-unicorn": "^48.0.1",
        "husky": "^8.0.0",
        "prettier": "^3.2.2",
        "prettier-plugin-astro": "^0.13.0",
        "start-server-and-test": "^2.0.3",
        "typescript": "^5.3.3"
    }
}
